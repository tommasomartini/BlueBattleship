<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.et.bluebattleship"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_blue"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.et.bluebattleship.Main_Blue_Battleship"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.et.bluebattleship.Campo"
            android:label="@string/title_activity_campo"
            android:background="@drawable/onda2" >
        </activity>
        <activity
            android:name="com.et.bluebattleship.Match_Enemy"
            android:label="@string/title_activity_match" 
            android:background="@drawable/onda2">
        </activity>
        
        <activity
            android:name="com.et.bluebattleship.Match_Win"
            android:label="@string/title_activity_match__win"
            android:background="@drawable/onda2" >
        </activity>
        <activity
            android:name="com.et.bluebattleship.My_Field"
            android:label="@string/title_activity_my__field" 
            android:background="@drawable/onda2">
        </activity>
        
    </application>

</manifest>
